{
    "manifest_version": 3,
    "name": "InsPop",
    "description": "播放电影台词海报",
    "version": "1.0",
    "permissions": ["tabs"],
    "optional_host_permissions": [
        "https://*/*"
    ],
    "chrome_url_overrides": {
        "newtab": "./index.html"
    },
    "icons": {
        "16": "./images/icon16.png",
        "32": "./images/icon32.png",
        "48": "./images/icon48.png",
        "128": "./images/icon128.png"
    }
}
